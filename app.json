{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "web": {
      "output": "server",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-router", { "origin": "http://localhost:8085" }]
    ],
    "name": "LOMA",
    "slug": "loma",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "bundleIdentifier": "com.anonymous.loma",
      "supportsTablet": true
    },
    "android": {
      "package": "com.anonymous.loma",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f5f5f5"
      }
    }
  }
}
